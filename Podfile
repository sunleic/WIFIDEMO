# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'WIFIDEMO' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  #use_frameworks!

  pod 'AFNetworking', '~> 3.0'
  pod 'Masonry'
  pod 'FMDB'
  pod 'MJRefresh'
  pod 'MJExtension'
  pod 'MBProgressHUD'
  
  # 腾讯开源系列
  pod 'WCDB'
  pod 'MMKV' # 微信开源键值存储系统
  
  #  pod 'YYKit' #不建议直接引入YYKit，里面的有很多函数没有yy前缀，容易冲突
  pod 'YYModel'
  pod 'YYText'
  pod 'YYCategories'
  


  pod 'WebViewJavascriptBridge', '~> 6.0'


  # Pods for WIFIDEMO

  target 'WIFIDEMOTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'WIFIDEMOUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
